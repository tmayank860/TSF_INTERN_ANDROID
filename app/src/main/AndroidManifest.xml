<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tmaya.tsf_spark_foundation">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.tmaya.tsf_spark_foundation.LoginActivity"
            android:label="@string/login_activity_label" />
        <activity
            android:name="com.example.tmaya.tsf_spark_foundation.SignUpActivity"
            android:label="@string/sign_up_activity_label" />
        <activity
            android:name="com.example.tmaya.tsf_spark_foundation.PersonalDetailActivity"
            android:label="@string/personal_details_activity_label" />
        <activity android:name="com.example.tmaya.tsf_spark_foundation.EducationalDetailsActivity" />
        <activity android:name="com.example.tmaya.tsf_spark_foundation.ViewPagerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>